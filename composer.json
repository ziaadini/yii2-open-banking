{
  "name": "ziaadini/yii2-open-banking",
  "description": "Yii2 Open Banking Service",
  "keywords": [
    "yii2",
    "framework",
    "extension",
    "Open Banking",
    "Bank"
  ],
  "homepage": "https://github.com/ziaadini/yii2-open-banking",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/ziaadini/yii2-open-banking/issues?state=open",
    "source": "https://github.com/ziaadini/yii2-open-banking"
  },
  "authors": [
    {
      "name": "Sadegh Shafii",
      "email": "sadshafiei.01@gmail.com",
      "homepage": "https://github.com/Sadi01"
    },
    {
      "name": "Amin Shafiei",
      "email": "amin.shafiee.d@gmail.com",
      "homepage": "https://github.com/aminshafieed"
    },
    {
      "name": "Parvaneh Seifaddini ",
      "email": "p.seifaddini.dev@gmail.com",
      "homepage": "https://github.com/pseifaddini"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "yiisoft/yii2": "~2.0.14"
  },
  "autoload": {
    "psr-4": { "sadi01\\openbanking\\": "src" }
  },  
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "extra": {
    "bootstrap": "sadi01\\openbanking\\Bootstrap"
  }
}
